{
    "dataRoles": [
        {
            "displayName": "categories",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "values",
            "name": "values",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 2
                    },
                    "value": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "values"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "Tooltips"
        ]
    },
    "privileges": []
}